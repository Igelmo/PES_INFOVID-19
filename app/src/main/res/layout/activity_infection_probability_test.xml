        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingStart="20dp"
        android:text="@string/P11Infection"
        app:layout_constraintBottom_toTopOf="@id/buttonComprovarProbabilitat"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <Button
        android:id="@+id/buttonComprovarProbabilitat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="generateInfectionResult"
        android:text="@string/string_button_comprovar_probabilitat"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />


</androidx.constraintlayout.widget.ConstraintLayout>
