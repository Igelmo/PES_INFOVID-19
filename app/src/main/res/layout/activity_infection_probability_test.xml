        android:id="@+id/switch5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:showText="true"
        android:textOff="NO"
        android:textOn="SI"
        app:layout_constraintBottom_toBottomOf="@id/textView5"
